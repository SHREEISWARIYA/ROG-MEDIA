{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/savedata",
            "dest": "app.js"
        },
        {
            "src": "/welcome",
            "dest": "app.js"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        }
    ]
}